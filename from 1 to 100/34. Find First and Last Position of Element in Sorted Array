class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int target) {
        vector<int> ans = {-1, -1};
        if(nums.empty())
            return ans;
        int  j = 0;
        int times = 0;
        for(int i = 0;i < nums.size() && nums[i] <= target;i++){
            if(nums[i] == target){
                if(i + 1 < nums.size() && nums[i] == nums[i+1])
                    times++;
                else
                ans[1] = i;
            }
        }
        ans[0] = ans[1] - times;
        return ans;
    }
};
