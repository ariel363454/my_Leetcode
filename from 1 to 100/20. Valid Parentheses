class Solution {
public:
    bool isValid(string s) {
        stack<char> chars;
        for(int i = 0;i < s.size();i++){
            if(s[i] == '(' || s[i] == '[' || s[i] == '{')
                chars.push(s[i]);
            else{
                if(chars.empty())
                    return false;
                if(chars.top() == '(' && s[i] != ')')
                    return false;
                if(chars.top() == '[' && s[i] != ']')
                    return false;
                if(chars.top() == '{' && s[i] != '}')
                    return false;
                chars.pop();
            }
        }
        if(chars.empty())
            return true;
        return false;
    }
};
